<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>rsy_bringup</name>
  <version>0.0.1</version>
  <description>Bringup package for RSY robot system</description>
  <maintainer email="alexander@todo.todo">Alexander</maintainer>
  <license>Apache-2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <exec_depend>launch</exec_depend>
  <exec_depend>launch_ros</exec_depend>
  <exec_depend>rviz2</exec_depend>
  <exec_depend>rsy_robot_startup</exec_depend>
  <exec_depend>rsy_moveit_startup</exec_depend>
  <exec_depend>rsy_path_planning</exec_depend>
  <exec_depend>rsy_cube_motion</exec_depend>
  <exec_depend>rsy_cube_perception</exec_depend>
  <exec_depend>rsy_gripper_controller</exec_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
